#include <stdio.h>

void main ()
{
	double x = 1.5 * 5;
	double y = 1.5 * 7;
	double z = 1.5 * 10;

	printf("millimeters higher in 5 years" , x);
	printf("millimeters higher in 7 years" , y);
	printf("millimeters higher in 10 years" , z);

}
